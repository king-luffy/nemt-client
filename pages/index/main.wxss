@import "../html/article/main.wxss";

radio {
    transform: scale(.8);
}

.center {
    background: #fff;
    position: relative;
}

.swip,.top-swiper {
    height: 300rpx;
}

.top-swiper {
    width: 100%;
}

.search_bg {
    position: absolute;
    left: 0;
    top: 165rpx;
    right: 0;
    height: 70rpx;
    text-align: center;
}

.search {
    width: 600rpx;
    display: inline-block;
    color: #000;
    background-color: #fff;
    opacity: .4;
    border-radius: 35rpx;
    line-height: 70rpx;
    font-size: 28rpx;
}

.search_img {
    display: inline;
    width: 36rpx;
    height: 36rpx;
    float: left;
    padding-top: 15rpx;
    padding-left: 20rpx;
}

.tabs {
    padding: 30rpx;
}

.tab-box {
    border-radius: 10rpx;
    box-shadow: 5rpx 5rpx 10rpx #999;
    overflow: hidden;
}

.tab-head {
    height: 80rpx;
    background: #f0f0f0;
    overflow: hidden;
}

.tab-head-ls {
    line-height: 80rpx;
    font-size: 30rpx;
    text-align: center;
    color: #3b9aff;
}

.tab-active {
    background: #3b9aff;
    color: #fff;
}

.tab-body {
    padding: 20rpx 30rpx;
    border: 1rpx solid #f0f0f0;
    border-top: 0rpx;
}

.bod-ls {
    height: 90rpx;
}

.bod-ls,.bod-lsn {
    line-height: 90rpx;
    font-size: 30rpx;
    overflow: hidden;
    border-bottom: 1rpx solid #f0f0f0;
}

.bod-lsn {
    height: 180rpx;
}

.bod-l {
    float: left;
    width: 200rpx;
}

.bod-l,.bod-r {
    font-size: 28rpx;
}

.bod-r {
    float: right;
    text-align: right;
    width: 400rpx;
}

.bod-int {
    float: right;
    height: 60rpx;
    line-height: 60rpx;
    margin-top: 15rpx;
}

.commt {
    padding: 30rpx 5rpx 10rpx;
}

.commt-btn {
    height: 80rpx;
    line-height: 80rpx;
    font-size: 28rpx;
}

.commt-btn,.commt-btn2 {
    color: #fff;
    text-align: center;
    background: #3b9aff;
}

.commt-btn2 {
    width: 116rpx;
    height: 41rpx;
    line-height: 40rpx;
    font-size: 24rpx;
    margin-left: 0rpx;
}

.commt-txt {
    height: 40rpx;
    line-height: 40rpx;
    padding-top: 10rpx;
    font-size: 18rpx;
    color: #a8a8a8;
    overflow: hidden;
}

.btx {
    color: #1296db;
}

.news {
    overflow: hidden;
    height: 40rpx;
    padding: 20rpx 0rpx 20rpx 30rpx;
    background-color: #f8f8f8;
    border-radius: 30rpx;
    margin-bottom: 10rpx;
}

.news-img {
    float: left;
    height: 40rpx;
    width: 68rpx;
    background: #1296db;
}

.news-swiper {
    height: 48rpx;
    width: 500rpx;
    float: left;
    padding-left: 20rpx;
}

.news-item {
    width: 100%;
    font-size: 28rpx;
    height: 48rpx;
    line-height: 48rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    color: #333;
}

.servce-title {
    padding: 30rpx;
    color: #333;
    font-size: 32rpx;
    font-weight: 700;
}

.serve-box {
    text-align: center;
    padding-bottom: 30rpx;
}

.serve-ls {
    display: inline-block;
    width: 210rpx;
    height: 300rpx;
    margin: 0 12rpx;
    border-radius: 18rpx;
    overflow: hidden;
}

radio .wx-radio-input.wx-radio-input-checked:before {
    border-radius: 50%;
    width: 49rpx;
    height: 49rpx;
    text-align: center;
    background-color: #3b9aff;
    transform: translate(-50%,-50%) scale(1);
}

.other-div {
    width: 200rpx;
    height: 30rpx;
    font-size: 20rpx;
    position: absolute;
    z-index: 10001;
    top: 50%;
    left: 50%;
    margin-top: -15rpx;
    margin-left: -100rpx;
    border-radius: 5rpx;
}

.bod-r-i {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
    align-items: center;
}

.bod-r-c {
    width: 130rpx;
}

.zhe {
    position: absolute;
    width: 140rpx;
    height: 80rpx;
    top: 57%;
    right: 5%;
    z-index: 1;
    background: transparent;
}

.show-btn {
    margin-top: 100rpx;
    color: #2c2;
}

.modal-mask {
    width: 100%;
    height: 100%;
    top: 0;
    background: #000;
    opacity: .5;
    z-index: 9000;
    color: #fff;
}

.modal-dialog,.modal-mask {
    position: fixed;
    left: 0;
    overflow: hidden;
}

.modal-dialog {
    width: 540rpx;
    top: 50%;
    z-index: 9999;
    background: #fff;
    margin: -180rpx 105rpx;
    border-radius: 36rpx;
}

.modal-title {
    padding-top: 50rpx;
    font-size: 36rpx;
    color: #030303;
    text-align: center;
}

.modal-content {
    padding: 50rpx 32rpx;
}

.modal-input {
    display: flex;
    background: #fff;
    border-radius: 4rpx;
    font-size: 28rpx;
}

.input {
    width: 100%;
    height: 82rpx;
    font-size: 28rpx;
    line-height: 28rpx;
    padding: 0 20rpx;
    box-sizing: border-box;
    color: #333;
}

input-holder {
    color: #666;
    font-size: 28rpx;
}

.modal-footer {
    display: flex;
    flex-direction: row;
    height: 86rpx;
    border-top: 1px solid #dedede;
    font-size: 34rpx;
    line-height: 86rpx;
}

.btn-cancel {
    color: #666;
}

.btn-cancel,.btn-confirm {
    width: 50%;
    text-align: center;
    border-right: 1px solid #dedede;
}

.btn-confirm {
    color: #ec5300;
}

.btn-confirm2 {
    width: 100%;
    color: green;
    font-weight: 700;
    text-align: center;
}

.new-bg {
    width: 100%;
    /* height: 600rpx; */
    background-color: #3b9aff;
}

.new-kao {
    width: 40%;
    height: 150rpx;
    color: #fff;
    position: absolute;
    left: 22rpx;
    top: 100rpx;
}

.new-gao,.new-kao {
    font-size: 30rpx;
}

.new-title {
    padding-top: 250rpx;
    text-align: center;
    color: #fff;
}

.title1 {
    display: flex;
    justify-content: center;
    align-items: center;
    font-weight: 700;
    font-size: 45rpx;
}

.title1-x {
    width: 30rpx;
    height: 10rpx;
    background-color: #0ff;
}

.title2 {
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 25rpx;
    line-height: 35rpx;
    padding-left: 30rpx;
}

.title2-x {
    width: 50rpx;
    height: 10rpx;
    background-color: #0ff;
}

.new-list {
    justify-content: center;
    padding-top: 10rpx;
}

.list,.new-list {
    display: flex;
    align-items: center;
}

.list {
    justify-content: space-around;
    flex-flow: row;
    width: 95%;
    height: 265rpx;
    background-color: #fff;
    border-radius: 30rpx;
    flex-wrap: wrap;
}

.li {
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    width: 18%;
}

.li image {
    width: 70rpx;
    height: 70rpx;
}

.li-title {
    font-size: 25rpx;
}

.info {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    color: #333;
    font-size: 32rpx;
    font-weight: 700;
}

.info-title {
    margin-left: 30rpx;
}

.info-b {
    border-bottom: 4rpx solid #3b9aff;
}

.box {
    padding: 0 30rpx;
}

.art {
    padding: 25rpx 0;
    height: 180rpx;
    border-bottom: 1rpx dashed #f0f0f0;
}

.art-img {
    float: left;
    width: 260rpx;
    height: 180rpx;
}

.art-txt {
    padding-left: 280rpx;
    padding-top: 10rpx;
}

.art-tt {
    font-size: 30rpx;
    line-height: 40rpx;
    max-height: 80rpx;
}

.art-ds,.art-tt {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.art-ds {
    color: #8e8e8e;
    padding-top: 15rpx;
    font-size: 24rpx;
    line-height: 35rpx;
    max-height: 70rpx;
}

.ad_new {
    display: none;
    width: 500rpx;
    margin-left: -250rpx;
    height: 700rpx;
    margin-top: -250rpx;
    position: fixed;
    top: 50%;
    left: 50%;
    border-radius: 20rpx;
    z-index: 99999;
    -webkit-animation-name: scaleDraw;
    -webkit-animation-timing-function: ease-in-out;
    -webkit-animation-iteration-count: 1;
    -webkit-animation-duration: .3s;
}

@-webkit-keyframes scaleDraw {
    0% {
        transform: scale(.5);
    }

    75% {
        transform: scale(1.1);
    }
}

@keyframes scaleDraw {
    0% {
        transform: scale(.5);
    }

    75% {
        transform: scale(1.1);
    }
}

.ad_close {
    background: red;
    border-radius: 50%;
    width: 44rpx;
    height: 44rpx;
    position: absolute;
    right: -12rpx;
    top: -12rpx;
    color: #fff;
    line-height: 40rpx;
    text-align: center;
}

.ad_img {
    width: 500rpx;
    height: 300rpx;
}

.ad_content {
    width: 500rpx;
    /* height: 600rpx; */
    background-color: #fff;
    border-radius: 20rpx;
    text-align: center;
    display: flex;
    align-items: center;
    flex-flow: column;
}

.ad_title {
    font-size: 40rpx;
    font-weight: 700;
    width: 80%;
    height: 60rpx;
    margin-top: 20rpx;
}

.ad_desc {
    font-size: 35rpx;
    width: 80%;
    height: 120rpx;
    margin-top: 10rpx;
}

.ad_botton {
    width: 200rpx;
    height: 60rpx;
    background-color: #14a1fd;
    border-radius: 5rpx;
    font-size: 35rpx;
    color: #fff;
    line-height: 50rpx;
}